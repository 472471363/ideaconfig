<templateSet group="output">
  <template name="syse" value="System.err.println(&quot;-------当前类：$CLASS_NAME$-------方法名为：$METHOD_NAME$----变量名：$VAL1$,值：&quot;+$VAL1$+&quot;-------&quot;);$END$" description="变量值打印" toReformat="false" toShortenFQNames="true">
    <variable name="CLASS_NAME" expression="className(sClassName)" defaultValue="" alwaysStopAt="false" />
    <variable name="METHOD_NAME" expression="methodName()" defaultValue="" alwaysStopAt="false" />
    <variable name="VAL1" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_CODE" value="true" />
    </context>
  </template>
</templateSet>